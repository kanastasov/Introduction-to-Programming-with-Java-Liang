package Exercises;

public class OccuranceOfSpecialCharacter_9_04 
{
	public static void main(String[] args) 
	{
		String str = "Welcome";
		System.out.println(count(str));

	}

	public static int count(String str, char a)
	{
		int count = 0;
		for(int i=0; i<str.length(); i++)
		{
			if(str.charAt(i) == a)
			{
				count++;
			}
		}
		return count;
	}
}
