package Exersices;

public class TicTacToe 
{
	public static void main(String[] args) 
	{
		for(int i=0; i<10; i++)
		{
			display();	
		}
		
	}

	public static void display()
	{
		String msg = "";
		int counter = 1;
		if(counter % 2 != 0)
		{
			msg = "First";
			System.out.println("First");
		}
		else
		{
			msg = "Second";
			System.out.println("Second");
		}
		counter++;
		
		
		System.out.println("-------------------------" +
			"\n|\t|\t|\t| " +
			"\n-------------------------" +
			"\n|\t|\t|\t| " +
			"\n-------------------------" +
			"\n|\t|\t|\t|" +
			"\n-------------------------");
		
		System.out.println("Enter a row(1, 2, or 3) for player " + isFirst());
	}
	
}
