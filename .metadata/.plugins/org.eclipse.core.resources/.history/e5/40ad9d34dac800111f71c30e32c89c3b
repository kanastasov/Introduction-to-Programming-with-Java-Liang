package Exercise;

import java.util.Scanner;
public class SortingThreeIntegers_08 
{
	public static void main(String[] args) 
	{
		Scanner input = new Scanner(System.in);
		System.out.println("Enter three integers: ");
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		
		if(a <= b && b <= c)
		{
			System.out.println(a + " " + b + " " + c);
		}
		
	}

}
