package Exercises;

public class Pyramid_4_17 
{
	public static void main(String[] args) 
	{
		int counter = 0;
		for(int i=0; i<7; i++)
		{
			for(int j=0; j<7; j++)
			{
				System.out.print(j + " ");
				counter++;
				if(counter == 7)
				{
					continue;
				}
				
			}
			System.out.println();
//			System.out.println(i);
		}
			
	}

}
