package Comprehensive;

public class PerfectNumber_4_33 
{
	public static void main(String[] args) 
	{
		
		int perfectNum = 5;
		int counter = 1;
		int temp = 0;
		while(perfectNum < 100 )
		{
			for(int i=0; i< 100; i++)
			{
				if(perfectNum % i == 0 && perfectNum != i)
				{
					temp += i;
					System.out.println(temp);
				}
				
			}
			
			
			perfectNum ++;
		}
		
		
		
	}

}
