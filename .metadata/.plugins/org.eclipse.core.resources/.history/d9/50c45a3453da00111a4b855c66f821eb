package Exercises;
import java.util.*;
public class Location_8_13
{
	
	public static Location locateLargest(double[][] a)
	{
		Random generator = new Random();
		double sum = 0;
		a = new double[3][4];
		for(int i=0; i<a.length; i++)
		{
			int index = 0;
			for(int j=0; j<a[i].length; j++)
			{
				a[i][j] = generator.nextDouble() * 10;
			}
		}
	}
	
}
